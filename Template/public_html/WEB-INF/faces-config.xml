<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
    <application>
        <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
        <!--Injector-->
        <el-resolver>com.tuyendev.inject.ELResolverWithInjectEJB</el-resolver>
        <!-- bundle -->
        <resource-bundle>
            <base-name>com.tuyendev.resources.template</base-name>
            <var>template</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>com.tuyendev.messagebundle.messages</base-name>
            <var>message-bundle</var>
        </resource-bundle>
        <!--Locale-->
        <locale-config>
            <default-locale>vi</default-locale>
            <supported-locale>en</supported-locale>
            <supported-locale>vi</supported-locale>
        </locale-config>
        <message-bundle>com.tuyendev.messagebundle.MessageBundle</message-bundle>
    </application>
</faces-config>